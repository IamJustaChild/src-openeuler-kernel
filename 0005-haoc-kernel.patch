+static int __init iee_si_copy_huge_pmd(struct mm_struct *mm, pmd_t *dst_pmd,
+			    pmd_t *src_pmd, unsigned long addr)
+{
+	pmd_t pmd;
+	int ret = -ENOMEM;
+
+	ret = -EAGAIN;
+	pmd = *src_pmd;
+	mm_inc_nr_ptes(mm);

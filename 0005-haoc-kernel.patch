+
+void *get_iee_pgtable_page(gfp_t gfpflags)
+{
+    struct page *page;
+    void *res = kmem_cache_alloc(pgtable_jar, gfpflags);
+    iee_set_freeptr((void **)((unsigned long)res + pgtable_jar_offset), NULL);

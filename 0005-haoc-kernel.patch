+static void set_init_iee_stack_page(unsigned long addr)
+{
+	unsigned long iee_addr = __phys_to_iee(__pa_symbol(addr));
+	pgd_t *pgdir = swapper_pg_dir;
+	pgd_t *pgdp = pgd_offset_pgd(pgdir, addr);
